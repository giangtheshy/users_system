<%users.forEach((user,index)=>{%>

  <tr>
    <td>
      <%=index+1%>
    </td>
    <td>
      <%=user.name%>
    </td>
    <td>
      <%=user.email%>
    </td>
    <td>
      <%=user.gender%>
    </td>
    <td>
      <%=user.status%>
    </td>
    <td>
      <a href="/update_user?id=<%=user._id%>" class="btn"><i class="fas fa-edit"></i></a>
      <a class="btn remove" data-id="<%=user._id%>"><i class="fas fa-trash"></i></a>
    </td>
  </tr>
  <%})%>